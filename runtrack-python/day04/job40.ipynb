   "source": [
    "def factorial(n) :\n",
    "    if n<0 :\n",
    "        raise ValueError(\"Value can't be below zero\")\n",
    "    elif n==0 :\n",
    "        return(1)\n",
    "    else :\n",
    "        return(n*factorial(n-1))"
   ]
  },
